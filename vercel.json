{
    "buildCommand": "npm run build && cd mobile && npm install && npx expo export --platform web && mv dist ../dist/mobile",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/mobile/(.*)",
            "destination": "/mobile/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}